/*Function Declaration List*/
int function compare_int(a:int, b:int)
    begin
        begin
            type int_b = int;
            var apple, banana : int := 0;
            var coconut: int;
            
            coconut := 1;
            
            if (a > b) then
                begin
                    apple := 1;
                end;
            else
                apple := 2;
            endif;
            
            return apple;
        end;
    end;

fixedpt function constant_add(a:fixedpt, b:fixedpt)
    begin
        begin
            while (a<100.0) do
                if (a>90.0) then
                    break;
                endif;
                a := a + b;
            enddo;
            
            return a;
        end;
    end;
    
int function sum_int_array(a:int_array, b:int_array)
    begin
        begin
            var sum : int := 0;
            var i : int := 0;
            for i := 1 to 3 do
                sum := sum + a[i];
                sum := sum + b[i];
            enddo;
            
            return sum;
        end;
    end;

int function sum_double_array(a:int_double_array, b:int_double_array)
    begin
        begin
            var sum : int := 0;
            for i := 1 to 2 do
                sum := sum + a[i][i+1];
                sum := sum + b[i][i+1];
            enddo;
            
            return sum;
        end;
    end;
    
